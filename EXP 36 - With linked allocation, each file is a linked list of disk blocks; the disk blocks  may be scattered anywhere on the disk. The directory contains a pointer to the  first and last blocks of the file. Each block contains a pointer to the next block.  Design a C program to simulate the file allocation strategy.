#include <stdio.h>
#include <string.h>

typedef struct { char d[50]; int n; } B;
B disk[50]; int f=-1,l=-1;

int a(){for(int i=0;i<50;i++) if(!disk[i].d[0]) return i; return -1;}

int main(){
    int b;
    b=a(); strcpy(disk[b].d,"Hello"); disk[b].n=-1; f=l=b;
    b=a(); strcpy(disk[b].d,"OS"); disk[b].n=-1; disk[l].n=b; l=b;
    b=a(); strcpy(disk[b].d,"File"); disk[b].n=-1; disk[l].n=b; l=b;

    for(int c=f;c!=-1;c=disk[c].n) printf("%s -> ",disk[c].d);
    printf("NULL\n");
}
